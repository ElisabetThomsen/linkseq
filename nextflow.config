
params {
    multiqc_config = "exolink/multiqc_config.yaml"
}

process {
    executor = 'local'
    memory = '16GB'
    cpus = 1
}

executor {
    name = 'local'
    cpus = 1
    memory = '16GB'
}

// Capture exit codes from upstream processes when piping.
process.shell = ['/bin/bash', '-euo', 'pipefail']
